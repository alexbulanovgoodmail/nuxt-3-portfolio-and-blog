{
	"name": "nuxt-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
		"lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
